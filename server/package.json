{
  "name": "keepit",
  "version": "0.0.1",
  "description": "A simple web app to create, edit and structure notes.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier \"./lib/*.js\" --write && prettier \"*.js\" --write && prettier \"client/src/**/*.{js,html,css}\" --write",
    "start:client": "npm run start --prefix ../client",
    "start:server": "nodemon server.js",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "lint:server": "eslint \"*.js\" && eslint \"./lib/*.js\"",
    "lint:client": "eslint client/src/**/*.js --quiet",
    "lint": "npm run lint:server && npm run lint:client",
    "check": "npm run format && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-toscani/keepit.git"
  },
  "keywords": [
    "notes"
  ],
  "author": "Julian Toscani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-toscani/keepit/issues"
  },
  "homepage": "https://github.com/j-toscani/keepit#readme",
  "devDependencies": {
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.4.1"
  }
}