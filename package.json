{
  "name": "keepit",
  "version": "0.1.0",
  "description": "A simple web app to create, edit and structure notes.",
  "main": "client/src/index.js",
  "scripts": {
    "format": "prettier \"./lib/*.js\" --write && prettier \"*.js\" --write && prettier \"client/src/**/*.{js,html,css}\" --write",
    "lint": "npm run lint --prefix client && npm run lint --prefix server",
    "start:server": "npm run start --prefix server",
    "start:client": "npm run start --prefix client",
    "install:client": "cd client && npm install && cd ../ ",
    "install:server": "cd server && npm install && cd ../",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "storybook": "npm run storybook --prefix client",
    "build": "concurrently \"npm run build --prefix client\" \"npm run start:server\"",
    "install": "npm run install:client && npm run install:server",
    "start": "npm run install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-toscani/keepit.git"
  },
  "keywords": [
    "notes",
    "react",
    "node"
  ],
  "author": "Julian Toscani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-toscani/keepit/issues"
  },
  "homepage": "https://github.com/j-toscani/keepit#readme",
  "dependencies": {
    "concurrently": "^5.1.0"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  }
}
